 {
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
      "semi": ["error", "always"],
      "prefer-const": "error",
      "quotes" :["error" , "single"]
    }
  }